{
  "name": "capacitor-flashlight",
  "version": "0.0.2",
  "description": "Capacitor plugin for using the flashlight of your device",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Daniel Sogl",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "@capacitor/android": "latest",
    "@capacitor/ios": "latest",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorFlashlight.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danielsogl/capacitor-flashlight"
  },
  "bugs": {
    "url": "https://github.com/danielsogl/capacitor-flashlight/issues"
  }
}
